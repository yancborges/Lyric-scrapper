{
	"manifest_version": 2,
	"name": "Anta Lyrcis",
	"version": "0.0",
	"content_scripts": [
		{
			"matches": [
				"https://www.youtube.com/*"
			],
			"js": ["content.js","scripts.js"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": ["tabs"]
}